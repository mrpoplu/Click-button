{
    "manifest_version": 3,
    "name": "Automation Extension",
    "version": "1.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["https://*.roblox.com/communities/5211428/("], 
        "js": ["content.js"]
      }
    ]
  }
  
